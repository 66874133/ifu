<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	<display-name>crawler</display-name>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath:resources/applicationContext.xml</param-value>
	</context-param>
	<servlet>
		<servlet-name>svcReqReciveServlet</servlet-name>
		<servlet-class>com.funnel.svc.web.SvcReqReciveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>svcReqReciveServlet</servlet-name>
		<url-pattern>/service/*</url-pattern>
	</servlet-mapping>
</web-app>